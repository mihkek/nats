{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.2.10",
    "ext-curl": "*",
    "ext-json": "*",
    "barryvdh/laravel-dompdf": "^0.8.7",
    "convertapi/convertapi-php": "^1.1",
    "dimsav/laravel-translatable": "^10.0",
    "dompdf/dompdf": "^0.8.2",
    "fideloper/proxy": "~4.0",
    "intervention/image": "^2.5",
    "kordy/ticketit": "^0.4.9",
    "laravel/framework": "5.8.*",
    "laravel/passport": "^7.0",
    "laravel/tinker": "~1.0",
    "maatwebsite/excel": "^3.1",
    "mariuzzo/laravel-js-localization": "^1.6",
    "phpoffice/phpspreadsheet": "^1.5",
    "phpoffice/phpword": "^0.15.0",
    "web-token/jwt-core": "^2.1",
    "web-token/jwt-key-mgmt": "^2.1",
    "web-token/jwt-signature": "^2.1",
    "web-token/jwt-signature-algorithm-hmac": "^2.1",
    "zendframework/zend-stdlib": "^3.2"
  },
  "require-dev": {
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~6.0",
    "symfony/thanks": "^1.0"
  },
  "autoload": {
    "classmap": [
    ],
    "psr-4": {
      "App\\": "app/",
      "DJEM\\": "vendor/atarka/djemiii/app/",
      "Mirronix\\LogGlobal\\": "packages/mirronix/logglobal/",
      "Mirronix\\FieldAccess\\": "packages/mirronix/fieldaccess/",
      "Mirronix\\TranslationManager\\": "packages/mirronix/translationmanager/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "DJEM\\": "vendor/atarka/djemiii/app/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "symfony/thanks": true
    }
  }
}
